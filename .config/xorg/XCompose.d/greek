## -*- conf -*-

# Lowercase
<Multi_key> <a> <l> <p> : "α"
<Multi_key> <b> <e> <t> : "β"
<Multi_key> <g> <a> <m> : "γ"
<Multi_key> <d> <e> <l> : "δ"
