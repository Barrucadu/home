<Multi_key> <m> <u>     : "μ"
<Multi_key> <n> <u>     : "ν"
<Multi_key> <x> <i>     : "ξ"
<Multi_key> <o> <m> <i> : "ο"
<Multi_key> <p> <i>     : "π"
<Multi_key> <r> <h> <o> : "ρ"
<Multi_key> <s> <t> <i> : "ς"
<Multi_key> <s> <i> <g> : "σ"
<Multi_key> <t> <a> <u> : "τ"
<Multi_key> <u> <p> <s> : "υ"
<Multi_key> <p> <h> <i> : "φ"
<Multi_key> <c> <h> <i> : "χ"
<Multi_key> <p> <s> <i> : "ψ"
<Multi_key> <o> <m> <e> : "ω"

